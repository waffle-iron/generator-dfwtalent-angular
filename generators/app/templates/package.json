  "scripts": {
    "start": "nodemon --ignore client --exec 'LOG_LEVEL=trace node' server/index.js & npm run serve:dev",
    "build:dev": "rimraf dist && webpack --progress",
    "serve:dev": "webpack-dev-server --inline --progress",
    "build": "rimraf dist && NODE_ENV='production' webpack --config config/webpack.prod.js -p --progress --bail && npm run copy",
    "copy": "webpack --config config/webpack.copy.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "karma start karma.conf.js",
    "test:server": "NODE_ENV='test' mocha server/test",
    "e2e": "protractor protractor.config.js",
    "test:e2e": "npm run kill && (node server/index.js & npm run serve:dev & protractor protractor.config.js)",
    "webdriver:update": "webdriver-manager update",
    "kill": "kill -9 $(lsof -i tcp:8080) >> /dev/null 2>&1 || exit 0",
    "docker": "npm run build && docker build -t localhost:5000/dfwtalent/ui:latest .",
    "prod": "npm run build && cd dist && npm i --production && node server/index.js"
  },
